<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rt.base.framework.mapper.MqErrorRecordMapper">
    <insert id="insertMqErrorRecord" parameterType="com.rt.base.framework.entity.MqErrorRecord">
        insert into mq_error_record(message_id,message_content,error_reason,create_user_id,create_time)
        values (#{messageId},#{messageContent},#{errorReason},#{createUserId},#{createTime})
    </insert>
</mapper>
